{
  "name": "concretecms/oauth2-concretecms",
  "description": "concrete5 OAuth2 Client Provider for PHP League OAuth2-Client",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Embler",
      "email": "andrew@concrete5.org",
      "homepage": "http://andrewembler.com"
    }
  ],
  "require": {
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Concrete\\OAuth2\\Client\\": "src/"
